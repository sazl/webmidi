{
  "name": "bucklescript-tea-starter-kit",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "type": "dev",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "refmt": 3,
  "bs-dependencies": [
    "bucklescript-tea",
    "bs-webworkers"
  ],
  "bsc-flags": [
    "-bs-super-errors -no-alias-deps",
    "-color",
    "always",
    "-w -23"
  ]
}